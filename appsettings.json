{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HttpsRedirection": {
    "HttpsPort": 7184
  },
  "JwtSettings": {
    "Authority": "http://localhost:8080/realms/myrealm",
    "ValidIssuer": "http://localhost:8080/realms/myrealm",
    "ValidAudiences": [ "ovh", "account" ],
    "RequireHttpsMetadata": false
  }
}
